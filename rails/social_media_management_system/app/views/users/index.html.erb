<%= link_to "new user", new_user_path %>
<h1>User</h1>

<table class="Summary">
  <tr>
  <th>Name</th>
  <th>Gender</th>
  <th>Country</th>
  </tr>
<% @user.each do |user| %>
  <tr>
  <td><%= user.name %></td>
  <td><%= user.gender %></td>
  <td><%= user.country %></td>
  <td class="actions">
    <%= link_to("Show", user_path(user) ) %>
    <%= link_to("Edit", edit_user_path(user) ) %>
    <%= link_to("Delete", user_path(user), :method => :delete )%>
  </tr>

<% end %>
</table>
