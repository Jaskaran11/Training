<% title "Products" %>
<%= form_tag(products_path, method: :get, :id => "products_search", remote: true) do %>
  <p>
    <%= text_field_tag :search, params[:search] %>
    <%= submit_tag "Search" %>
  </p>
  <div id="products"><%= render 'products' %></div>
    <% end %>

<p><%= link_to "New Product", new_product_path %></p>